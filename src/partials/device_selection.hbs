        </div>
      </div>
      <div class="tile-button__content-container">
        <div class="flex-row flex-row--align-stretch flex-row--justify-start flex-row--flex-direction-column flex-row--column-spacing-sm"
             data-id="flex-row">
          {{#if device.nickname}}
            <div class="tile-button__title">{{device.nickname}}</div>
          {{else}}
            {{#ifEq device.type "Android"}}
              <div class="tile-button__title">PingOne Authenticator</div>
            {{else ifEq device.type "iPhone"}}
              <div class="tile-button__title">PingOne Authenticator</div>
            {{else ifEq device.type "TOTP"}}
              <div class="tile-button__title">Authenticator App</div>
            {{else ifEq device.type "SMS"}}
              <div class="tile-button__title">Text Message</div>
            {{else ifEq device.type "Email"}}
              <div class="tile-button__title">Email</div>
            {{else ifEq device.type "SECURITY_KEY"}}
              <div class="tile-button__title">Security Key</div>
            {{else ifEq device.type "PLATFORM"}}
              <div class="tile-button__title">Biometrics</div>
            {{else}}
              <div class="tile-button__title">{{device.type}}</div>
            {{/ifEq}}
          {{/if}}
          {{#ifEq device.type "Android"}}
            <div class="tile-button__content">{{device.name}}</div>
          {{else ifEq device.type "iPhone"}}
            <div class="tile-button__content">{{device.name}}</div>
          {{else if device.target}}
            <div class="tile-button__content">{{device.target}}</div>
          {{/ifEq}}
        </div>
      </div>
    </div>
     {{/isValidRpId}}
  {{/if}}
{{/each}}
